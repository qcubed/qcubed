<plugin>
	<name><![CDATA[QCaptchaTextBox]]></name>
	<description><![CDATA[Render a textbox with Captcha Image to prevent Spam bot to hack a form.]]></description>
	<version>1.1</version>
	<platform_version>2.0.2</platform_version>
	<author name="Mike Quirion" email="mikequirion [at] gmail [dot] com"/>
	<files>
		<file type="control" filename="includes/QCaptchaTextBox.class.php"/>
		<file type="control" filename="includes/QCaptchaImage.class.php"/>
		<file type="control" filename="includes/QCaptchaFilters.class.php"/>
		<file type="control" filename="fonts/bluehigl.ttf"/>
		<file type="control" filename="fonts/typewriterA602.ttf"/>
		<file type="image" filename="images/errorsign.jpg"/>
		<file type="image" filename="captcha.php"/>
		<file type="example" filename="example/QCaptchaTextBoxExample.php"/>
		<file type="example" filename="example/QCaptchaTextBoxExample.tpl.php"/>
	</files>
	<includes>
		<include_files classname="QCaptchaTextBox" filename="includes/QCaptchaTextBox.class.php"/>
		<include_files classname="QCaptchaImage" filename="includes/QCaptchaImage.class.php"/>
		<include_files classname="QCaptchaFilters" filename="includes/QCaptchaFilters.class.php"/>
	</includes>
	<examples>
		<example filename="example/QCaptchaTextBoxExample.php" description="QCaptchaTextBox: Simple example using QCaptcha Textbox"/>
	</examples>
</plugin>

